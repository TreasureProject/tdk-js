{
  "name": "@treasure-dev/tdk-api",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "pnpm generate && pnpm typecheck && tsup",
    "generate": "prisma generate",
    "dev": "pnpm generate && tsup --watch --onSuccess 'pnpm start'",
    "start": "node ./dist/index.js",
    "db:seed": "tsx ./prisma/seed.ts",
    "db:seed:dev": "pnpm db:seed dev",
    "db:seed:prod": "pnpm db:seed prod",
    "db:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.507.0",
    "@fastify/cors": "^10.0.0",
    "@fastify/swagger": "^9.0.0",
    "@fastify/swagger-ui": "^5.0.1",
    "@fastify/type-provider-typebox": "^5.0.0",
    "@prisma/client": "^5.3.0",
    "@sentry/node": "^8.7.0",
    "@sinclair/typebox": "^0.33.7",
    "@thirdweb-dev/engine": "^0.0.15",
    "@treasure-dev/auth": "*",
    "@treasure-dev/tdk-core": "*",
    "@wagmi/core": "^2.9.1",
    "abitype": "^1.0.5",
    "dotenv": "^16.3.1",
    "fastify": "^5.0.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "thirdweb": "^5.48.1",
    "viem": "^2.5.0"
  },
  "devDependencies": {
    "prisma": "^5.3.0"
  },
  "engines": {
    "node": ">=20"
  }
}
