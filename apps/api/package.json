{
  "name": "@treasure-dev/tdk-api",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run generate && tsup",
    "generate": "prisma generate",
    "dev": "npm run build && concurrently \"npm run build -- --watch\" \"nodemon -w ./dist/index.js ./dist/index.js\"",
    "start": "node ./dist/index.js",
    "db:seed": "tsx ./prisma/seed.ts",
    "db:seed:dev": "npm run db:seed dev",
    "db:seed:prod": "npm run db:seed prod",
    "db:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.507.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@prisma/client": "^5.3.0",
    "@sinclair/typebox": "^0.32.5",
    "@thirdweb-dev/auth": "^4.1.4",
    "@thirdweb-dev/engine": "^0.0.5",
    "@treasure-dev/tdk-core": "*",
    "@wagmi/core": "^2.9.1",
    "abitype": "^1.0.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "jwt-decode": "^4.0.0",
    "thirdweb": "^5.15.0",
    "viem": "^2.5.0"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "nodemon": "^3.0.1",
    "prisma": "^5.3.0"
  },
  "engines": {
    "node": ">=20"
  }
}
