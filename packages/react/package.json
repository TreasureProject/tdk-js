{
  "name": "@treasure-dev/tdk-react",
  "version": "1.5.0",
  "description": "Treasure Development Kit for React-based projects",
  "keywords": [
    "treasure",
    "tdk",
    "bridgeworld",
    "arbitrum"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TreasureProject/tdk-js.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/TreasureProject/tdk-js/issues",
    "email": "engineering@treasure.lol"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "tsup",
    "generate": "icons build -i ./icons -o ./src/icons --optimize",
    "dev": "npm run build -- --watch",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@tanstack/react-query": "^5.18.1",
    "@treasure-dev/tdk-core": "*",
    "clsx": "^2.1.1",
    "i18next": "^23.6.0",
    "react": "^18.2.0",
    "react-i18next": "^13.3.1",
    "react-verification-input": "^4.1.2",
    "tailwind-merge": "^2.5.2",
    "thirdweb": "^5.45.1",
    "viem": "^2.7.6",
    "wagmi": "^2.5.6"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/react": "^8.2.8",
    "@storybook/react-vite": "^8.2.8",
    "@treasure-dev/tailwind-config": "*",
    "@types/react": "^18.2.24",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "storybook": "^8.2.8",
    "svg-icons-cli": "^0.0.8",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
