{
  "name": "@treasure-dev/launcher",
  "version": "1.1.2",
  "description": "Treasure Launcher utilities for the Treasure ecosystem",
  "keywords": [
    "treasure"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TreasureProject/tdk-js.git",
    "directory": "packages/launcher"
  },
  "bugs": {
    "url": "https://github.com/TreasureProject/tdk-js/issues",
    "email": "engineering@treasure.lol"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "test": "vitest run",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@treasure-dev/tdk-core": "workspace:*",
    "thirdweb": "catalog:"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
