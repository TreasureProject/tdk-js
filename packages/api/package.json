{
  "name": "@treasure/tdk-api",
  "version": "1.0.0",
  "module": "./dist/sdk.mjs",
  "types": "./dist/sdk.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "npm run build && concurrently \"npm run build -- --watch\" \"nodemon -w ./dist/index.js ./dist/index.js\"",
    "start": "node ./dist/index.js",
    "db:seed": "npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' ./prisma/seed.ts",
    "db:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^1.10.1",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@prisma/client": "^5.3.0",
    "@sinclair/typebox": "^0.31.21",
    "@thirdweb-dev/engine": "^0.0.1",
    "@treasure/tdk-core": "*",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "nodemon": "^3.0.1",
    "prisma": "^5.3.0"
  }
}
